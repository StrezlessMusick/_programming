<%= @comment.to_s.gsub(/^/,"* ") %>

&Process
  processId	= <%=   @processId	 %>
  run_tag	= '<%=   @run_tag	 %>'
  sqs0     	= <%=  "%.15g" % @sqs0     	 %>
  beams	        = <%=   @beams*" "	         %>

  PDFSet	= '<%=   @pdfSet	 %>'
  PDFMember	= <%=   @pdfMember	 %>

  ! electroweak flags
  !               iqed, iew, iborn, ifgg, irun, iph
  iflew         = <%=   @iflew*" "          %>
  ! qcd flags
  iflqcd        = <%=   @iflqcd         %>
  ! subtraction scheme 0/1/2 : none/MSbar/DIS
  imsb		= <%=   @imsb		 %>
  ! collinear particle recombination in calorimeter
  irecomb       = <%=   @irecomb        %>
/

&VegasPar
  relAcc	= <%=  "%.15g" % @relAcc	 %>
  absAcc	= <%= "%.15g" %  @absAcc	 %>
  nStart	= <%=   @nStart	 %>
  nIncrease	= <%=   @nIncrease	 %>
  nExplore	= <%=   @nExplore	 %>
  maxEval	= <%=   @maxEval	 %>
  seed	        = <%=   @seed	         %>
  flags 	= <%=   @flags 	 %>
/

&KinCuts
  cutName	= <%=   @cutName.map{|name| "'"+name+"'" }*","	 %>
  cutFlag       = <%=   @cutFlag*","        %>
  cutLow	= <%=   @cutLow.map{|e| "%.15g" % e}*","	 %>
  cutUp		= <%=   @cutUp.map{|e| "%.15g" % e}*","		 %>
/

! particle numbering 1+2 -> 3+4+5+... ( FIXME )
&FixedBinHist
  fbh_name	= <%=   @fbh_name.map{|name| "'"+name+"'" }*","	 %>
  fbh_flag	= <%=   @fbh_flag*"," %>
  fbh_low	= <%=   @fbh_low.map{|e| "%.15g" % e}*","	 %>
  fbh_up	= <%=   @fbh_up.map{|e| "%.15g" % e}*","	 %>
  fbh_step	= <%=   @fbh_step.map{|e| "%.15g" % e}*","	 %>
/

&VarBinHist
  nvbh         	= <%=   @nvbh         	 %>

<% @nvbh.times do |i| i+=1 %>
  vbh_name( <%=i%>)	= <%=   "'"+@vbh_name[i]+"'"	 %>,
  vbh_flag( <%=i%>)	= <%=   @vbh_flag[i]	 %>,
  vbh_nbins(<%=i%>)	= <%=   @vbh_bins[i].size-1	 %>,
  vbh_bins( <%=i%>,  1:<%=   @vbh_bins[i].size %>) = <%=   @vbh_bins[i].map{|e| "%.15g" % e}*" "  %>,

<% end %>

/
