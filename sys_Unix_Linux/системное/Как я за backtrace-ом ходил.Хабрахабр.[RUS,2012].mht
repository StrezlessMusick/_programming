.company_header .worker.leave{background: url('/i/company-work.gif') no-repeat left -20px;}
.company_header .fan{margin-left:27px;margin-top:16px;cursor:pointer;float:left;display:block;width:30px;background: url('/i/company-fav.gif') no-repeat;height:19px;}
.company_header .fan.unfan{background: url('/i/company-fav.gif') no-repeat left -19px;}
