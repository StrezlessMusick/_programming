.company_header .name{float:left;margin-right:26px;}
.company_header .name a{color:#343434;font-size:36px;text-decoration: none;}
.company_header .rating{float:left;color:#C6C;font-size:36px;position:relative;}
.company_header .rating .label{color:#CCC;font-size:12px;position:absolute;top:-10px;left:0;right:0;text-align:center;}
